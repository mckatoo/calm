{
  "name": "calm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:pull": "dotenv -e .env.local -- npx prisma db pull",
    "prisma:push": "dotenv -e .env.local -- npx prisma db push",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:migrate:reset": "dotenv -e .env.local -- npx prisma migrate reset",
    "test:watch": "NODE_ENV=development jest --watch",
    "test": "NODE_ENV=development jest",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@prisma/client": "3.10.0",
    "bcrypt": "5.0.1",
    "binance": "2.0.27",
    "image-downloader": "4.1.0",
    "jsonwebtoken": "8.5.1",
    "next": "12.1.0",
    "next-auth": "4.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.0",
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "12.1.4",
    "@types/bcrypt": "5.0.0",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "autoprefixer": "10.4.2",
    "dotenv-cli": "5.0.0",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "jest": "27.5.1",
    "postcss": "8.4.8",
    "prisma": "3.10.0",
    "tailwindcss": "3.0.23",
    "typescript": "4.6.2"
  }
}
